datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int      @id @default(autoincrement())
  discordId  String   @unique
  balance    Int      @default(1000)
  lastDaily  DateTime?
  streak     Int      @default(0)
}
